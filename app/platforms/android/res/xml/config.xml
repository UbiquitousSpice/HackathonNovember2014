<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ubiquitousspice.checkyoself" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>CheckYoSelf</name>
    <description>
		A hackathon app made in November 2014 @ Fuse
	</description>
    <content src="index.html" />
    <access origin="*" />
    <log level="DEBUG" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ffcd34" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
</widget>
